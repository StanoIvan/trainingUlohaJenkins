pipeline {
    agent any
    stages {
        stage ('build') {
            steps {
                sh 'echo "buildujem jak sarkan"'
            }
        }
        stage ('test') {
            steps {
                sh 'echo "testujem ci bavi sarkan"'
            }
        }
        stage ('deploy') {
            steps {
                sh 'echo "ta ty vies pisat pipelines"'
            }
        }
    }
}
